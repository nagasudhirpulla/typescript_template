{
  "name": "electron_react_webpack_typescript_template",
  "version": "0.1.0",
  "private": false,
  "author": "Nagasudhir",
  "description": "Electron + Webpack + React + Typescript template",
  "main": "./dist/main.js",
  "homepage": "https://github.com/nagasudhirpulla/typescript_template",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react-poppop": "^1.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/electron": "^1.6.10",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-select": "^3.0.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "css-loader": "^2.1.1",
    "deepmerge": "^3.2.0",
    "electron": "^6.0.1",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.4",
    "electron-prebuilt": "^1.4.13",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.87.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "typings": "^2.1.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "build": {
    "appId": "electron_template_app",
    "asar": true,
    "productName": "Electron Template App",
    "files": [
      "dist/*"
    ],
    "directories": {
      "output": "pack_dist"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "assets/img/posoco_logo.png"
    }
  },
  "scripts": {
    "start": "./node_modules/.bin/electron dist/main.js",
    "build": "npm run build-client && npm run build-server",
    "build-server": "webpack --mode production --config webpack-server.config.js",
    "build-client": "webpack --mode production --config webpack-client.config.js",
    "pack": "rm -fr ./pack_dist && npm run electron-win-package",
    "electron-win-package": "electron-builder dist/main.js"
  }
}
